	/**
	 * @generated
	 */
	public static List<IotGmfNodeDescriptor> getArduino_1000SemanticChildren(
			View view) {
		if (!view.isSetElement()) {
			return Collections.emptyList();
		}
		Arduino modelElement = (Arduino) view.getElement();
		LinkedList<IotGmfNodeDescriptor> result = new LinkedList<IotGmfNodeDescriptor>();
		for (Iterator<?> it = modelElement.getMódulos().iterator(); it
				.hasNext();) {
			Módulo childElement = (Módulo) it.next();
			int visualID = IotGmfVisualIDRegistry.getNodeVisualID(view,
					childElement);
			if (visualID == MóduloEditPart.VISUAL_ID) {
				result.add(new IotGmfNodeDescriptor(childElement, visualID));
				continue;
			}
		}
		return result;
	}

