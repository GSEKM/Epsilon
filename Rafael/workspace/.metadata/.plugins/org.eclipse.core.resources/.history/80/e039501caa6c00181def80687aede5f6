@namespace(uri="http://iotgmf/1.0", prefix="gmf.diagram")

@gmf
package myModel;

@gmf.diagram(foo="bar")
class Arduino {
  op void setup();
  op void loop();
  attr String board;
}

abstract class Módulo {
  ref Arduino[*] Módulos;
}

class Motor extends Módulo {
  op void girar();
  attr int graus;
}

@gmf.node(label = "name")
class File {
	att String name;
}
