 * @generated
 */
public org.eclipse.swt.graphics.Image getImage(Object element) {
	if (element instanceof iot.diagram.navigator.IotNavigatorGroup) {
	iot.diagram.navigator.IotNavigatorGroup group = (iot.diagram.navigator.IotNavigatorGroup) element;
	return iot.diagram.part.IotDiagramEditorPlugin.getInstance().getBundledImage(group.getIcon());
}

	if (element instanceof iot.diagram.navigator.IotNavigatorItem) {
	iot.diagram.navigator.IotNavigatorItem navigatorItem = (iot.diagram.navigator.IotNavigatorItem) element;
	if (!isOwnView(navigatorItem.getView())) {
		return super.getImage(element);
	}
	return getImage(navigatorItem.getView());
}

	// Due to plugin.xml content will be called only for "own" views
if (element instanceof org.eclipse.core.runtime.IAdaptable) {
	org.eclipse.gmf.runtime.notation.View view = (org.eclipse.gmf.runtime.notation.View) ((org.eclipse.core.runtime.IAdaptable) element).getAdapter(org.eclipse.gmf.runtime.notation.View.class);
	if (view != null && isOwnView(view)) {
		return getImage(view);
	}
}

		return super.getImage(element);
}
	
		/**
 * @generated
 */
public org.eclipse.swt.graphics.Image getImage(org.eclipse.gmf.runtime.notation.View view) {
		switch (iot.diagram.part.IotVisualIDRegistry.getVisualID(view)) {
		case iot.diagram.edit.parts.BoardEditPart.VISUAL_ID:
	return getImage("Navigator?Diagram?iot?Board", iot.diagram.providers.IotElementTypes.Board_1000); //$NON-NLS-1$
case iot.diagram.edit.parts.ArduinoEditPart.VISUAL_ID:
	return getImage("Navigator?TopLevelNode?iot?Arduino", iot.diagram.providers.IotElementTypes.Arduino_2004); //$NON-NLS-1$
case iot.diagram.edit.parts.MotorEditPart.VISUAL_ID:
	return getImage("Navigator?TopLevelNode?iot?Motor", iot.diagram.providers.IotElementTypes.Motor_2005); //$NON-NLS-1$
case iot.diagram.edit.parts.ArduinoConectorEditPart.VISUAL_ID:
	return getImage("Navigator?Link?iot?Arduino?conector", iot.diagram.providers.IotElementTypes.ArduinoConector_4005); //$NON-NLS-1$
	}
		return getImage("Navigator?UnknownElement", null);  //$NON-NLS-1$
}
	
		/**
 * @generated
 */
private org.eclipse.swt.graphics.Image getImage(String key, org.eclipse.gmf.runtime.emf.type.core.IElementType elementType) {
	org.eclipse.jface.resource.ImageRegistry imageRegistry = iot.diagram.part.IotDiagramEditorPlugin.getInstance().getImageRegistry();
	org.eclipse.swt.graphics.Image image = imageRegistry.get(key);
	if (image == null && elementType != null && iot.diagram.providers.IotElementTypes.isKnownElementType(elementType)) {
		image = iot.diagram.providers.IotElementTypes.getImage(elementType);
		imageRegistry.put(key, image);
	}
			
	if (image == null) {
		image = imageRegistry.get("Navigator?ImageNotFound");  //$NON-NLS-1$
		imageRegistry.put(key, image);
	}
	return image;
}
	
		/**
 * @generated
 */
public String getText(Object element) {
	if (element instanceof iot.diagram.navigator.IotNavigatorGroup) {
	iot.diagram.navigator.IotNavigatorGroup group = (iot.diagram.navigator.IotNavigatorGroup) element;
	return group.getGroupName();
}

	if (element instanceof iot.diagram.navigator.IotNavigatorItem) {
	iot.diagram.navigator.IotNavigatorItem navigatorItem = (iot.diagram.navigator.IotNavigatorItem) element;
   		if (!isOwnView(navigatorItem.getView())) {
   			return null;
   		}
   		return getText(navigatorItem.getView());
	}

	// Due to plugin.xml content will be called only for "own" views
if (element instanceof org.eclipse.core.runtime.IAdaptable) {
	org.eclipse.gmf.runtime.notation.View view = (org.eclipse.gmf.runtime.notation.View) ((org.eclipse.core.runtime.IAdaptable) element).getAdapter(org.eclipse.gmf.runtime.notation.View.class);
	if (view != null && isOwnView(view)) {
		return getText(view);
	}
}

		return super.getText(element);
}
	
		/**
 * @generated
 */
public String getText(org.eclipse.gmf.runtime.notation.View view) {
	if (view.getElement() != null && view.getElement().eIsProxy()) {
		return getUnresolvedDomainElementProxyText(view);
	}
		switch (iot.diagram.part.IotVisualIDRegistry.getVisualID(view)) {
		case iot.diagram.edit.parts.BoardEditPart.VISUAL_ID:
	return getBoard_1000Text(view);
case iot.diagram.edit.parts.ArduinoEditPart.VISUAL_ID:
	return getArduino_2004Text(view);
case iot.diagram.edit.parts.MotorEditPart.VISUAL_ID:
	return getMotor_2005Text(view);
case iot.diagram.edit.parts.ArduinoConectorEditPart.VISUAL_ID:
	return getArduinoConector_4005Text(view);
	}
		return getUnknownElementText(view);
}
	
