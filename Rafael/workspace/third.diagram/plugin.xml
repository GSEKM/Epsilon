   <extension point="org.eclipse.ui.navigator.navigatorContent" id="navigator-content">
      <?gmfgen generated="true"?>
      <navigatorContent
            id="third.diagram.resourceContent" 
            name="%navigatorContentName" 
            priority="normal" 
            contentProvider="myModel.diagram.navigator.MyModelNavigatorContentProvider" 
            labelProvider="myModel.diagram.navigator.MyModelNavigatorLabelProvider"
            icon="icons/obj16/MyModelDiagramFile.gif"
            activeByDefault="true">
         <triggerPoints>
            <or>
	           <and>
    	          <instanceof value="org.eclipse.core.resources.IFile"/>
        	      <test property="org.eclipse.core.resources.extension" value="mymodel_diagram"/>
               </and>
               <instanceof value="myModel.diagram.navigator.MyModelAbstractNavigatorItem"/>
           	   <adapt type="org.eclipse.gmf.runtime.notation.View">
           	      <test property="third.diagram.isShortcut"/>
           	   </adapt>
            </or>
         </triggerPoints>
         <possibleChildren>
            <or>
         	   <instanceof value="myModel.diagram.navigator.MyModelAbstractNavigatorItem"/>
           	   <adapt type="org.eclipse.gmf.runtime.notation.View">
           	      <test property="third.diagram.isShortcut"/>
           	   </adapt>
            </or>
         </possibleChildren>
         <commonSorter 
               id="third.diagram.navigatorSorter" 
               class="myModel.diagram.navigator.MyModelNavigatorSorter">
            <parentExpression>
               <or>
	              <and>
    	             <instanceof value="org.eclipse.core.resources.IFile"/>
        	         <test property="org.eclipse.core.resources.extension" value="mymodel_diagram"/>
                  </and>
                  <instanceof value="myModel.diagram.navigator.MyModelAbstractNavigatorItem"/>
               </or>
            </parentExpression>
         </commonSorter>
      </navigatorContent>
      <navigatorContent
            id="third.diagram.domainResourceContent" 
            name="%domainNavigatorContentName" 
            priority="normal" 
            contentProvider="myModel.diagram.navigator.MyModelDomainNavigatorContentProvider" 
            labelProvider="myModel.diagram.navigator.MyModelDomainNavigatorLabelProvider"
            icon="icons/obj16/MyModelDiagramFile.gif"
            activeByDefault="true">
         <triggerPoints>
            <or>
	           <and>
    	          <instanceof value="org.eclipse.core.resources.IFile"/>
        	      <test property="org.eclipse.core.resources.extension" value="mymodel"/>
               </and>
               <instanceof value="myModel.diagram.navigator.MyModelDomainNavigatorItem"/>
            </or>
         </triggerPoints>
         <possibleChildren>
            <instanceof value="myModel.diagram.navigator.MyModelDomainNavigatorItem"/>
         </possibleChildren>
      </navigatorContent>
      <actionProvider
            id="third.diagram.navigatorActionProvider"
            class="myModel.diagram.navigator.MyModelNavigatorActionProvider">
         <enablement>
            <or>
               <instanceof value="myModel.diagram.navigator.MyModelAbstractNavigatorItem"/>
           	   <adapt type="org.eclipse.gmf.runtime.notation.View">
           	      <test property="third.diagram.isShortcut"/>
           	   </adapt>
            </or>
         </enablement>
      </actionProvider>
