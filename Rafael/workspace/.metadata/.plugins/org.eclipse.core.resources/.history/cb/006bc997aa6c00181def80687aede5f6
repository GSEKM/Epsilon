@namespace(uri="http://iotgmf/1.0", prefix="gmf.diagram")

@gmf
package myModel;

@gmf.diagram(foo="bar")
class Arduino {
  op void setup();
  op void loop();
  attr String board;
}

abstract class Módulo {
  ref Arduino[*] Módulos;
}

class Motor extends Módulo {
  op void girar();
  attr int graus;
}
@gmf.node(label="name")
@gmf.link(label="test",source="source",target="target",style="dot")






class File {
	att String name;
}
